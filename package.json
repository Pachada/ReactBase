{
  "name": "reactbase",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "jest --config jest.config.ts --passWithNoTests",
    "test:watch": "jest --config jest.config.ts --watch",
    "lint": "npm run lint:format && npm run lint:eslint",
    "lint:fix": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" && eslint ./src --ext .js,.jsx,.ts,.tsx --fix",
    "lint:format": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "lint:eslint": "eslint ./src --ext .js,.jsx,.ts,.tsx --max-warnings=0",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.511.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.56.4",
    "react-router-dom": "^7.6.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@tailwindcss/vite": "^4.1.12",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.16.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "@vitejs/plugin-react": "^4.6.0",
    "babel-jest": "^30.0.5",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "pre-commit": "^1.2.2",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.12",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^7.0.0"
  },
  "pre-commit": [
    "lint"
  ]
}
